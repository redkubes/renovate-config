{
  "extends": ["config:base"],
  "hostRules": [
    {
      "encrypted": {
        "token": ""
      },
      "hostType": "npm",
      "matchHost": "https://npm.pkg.github.com/"
    },
    {
      "encrypted": {
        "password": ""
      },
      "hostType": "docker",
      "matchHost": "docker.io",
      "username": "otomi"
    }
  ],
  "major": {
    "enabled": false
  },
  "npm": {
    "packageRules": [
      {
        "groupName": "all-npm",
        "matchPackagePatterns": ["*"]
      }
    ]
  },
  "packageRules": [
    {
      "automerge": true,
      "automergeType": "branch",
      "matchUpdateTypes": ["minor", "patch", "pin", "digest"]
    }
  ]
}
