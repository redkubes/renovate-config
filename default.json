{
  "extends": ["config:base"],
  "hostRules": [
    {
      "encrypted": {
        "token": ""
      },
      "hostType": "npm",
      "matchHost": "https://npm.pkg.github.com/"
    }
  ],
  "major": {
    "enabled": false
  },
  "packageRules": [
    {
      "automerge": true,
      "automergeType": "branch",
      "matchUpdateTypes": ["minor", "patch", "pin", "digest"]
    }
  ]
}
